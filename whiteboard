     public int[] rotateLeft3(int[] nums) {
        int[] umsn = new int[3];
         for (int i = 1; i < nums.length; i++){
             umsn[i-1] = nums[i];
         }
         umsn[2] = nums[0];
         return  umsn;
     }
 

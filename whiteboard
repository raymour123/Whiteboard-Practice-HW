public boolean cigarParty(int cigars, boolean isWeekend) {
  if (cigars && isWeekend >= 40){
    return true;
  }
    else if (cigars >= 40 && cigars <= 60);
        return true;
      
    }
    return false;
}

public String doubleChar(String str) {
         String out = "";
         for(int i = 0; i < str.length(); i++){
             out = out + str.charAt(i);
             out = out + str.charAt(i);
         }
         return out;
     }

